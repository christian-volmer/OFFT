          "\[IndentingNewLine]", 
          RowBox[{"Throw", "[", "False", "]"}], ";"}]}], 
        "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"Equal", "@@", 
           RowBox[{"BilinearFormLengths", "[", "blf", "]"}]}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Message", "[", 
           RowBox[{"TransposeBilinearForm", "::", "fail"}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"Throw", "[", "False", "]"}], ";"}]}], 
