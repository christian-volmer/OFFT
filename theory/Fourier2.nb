Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"RaderAlgorithm", "[", 
    RowBox[{"length_", ",", "convolution_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "g", ",", "gi", ",", "inputSequence", ",", "outputSequence", ",", 
