      RowBox[{"g", "=", 
       RowBox[{"PrimitiveRoot", "[", "length", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"gi", "=", 
       RowBox[{"PowerMod", "[", 
        RowBox[{"g", ",", 
         RowBox[{"-", "1"}], ",", "length"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"inputSequence", "=", 
       RowBox[{"PowerMod", "[", 
        RowBox[{"g", ",", 
         RowBox[{"Range", "[", 
          RowBox[{"0", ",", 
           RowBox[{"length", "-", "2"}]}], "]"}], ",", "length"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"outputSequence", "=", 
       RowBox[{"PowerMod", "[", 
        RowBox[{"gi", ",", 
         RowBox[{"Range", "[", 
          RowBox[{"0", ",", 
           RowBox[{"length", "-", "2"}]}], "]"}], ",", "length"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"inputPermutation", "=", 
       RowBox[{
        RowBox[{"IdentityMatrix", "[", 
