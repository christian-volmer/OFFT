                    RowBox[{"Abs", "[", 
                    RowBox[{"Drop", "[", 
                    RowBox[{"#", ",", 
                    RowBox[{"i", "-", "1"}]}], "]"}], "]"}], ",", "width"}], 
                    "]"}], "==", 
                    RowBox[{"UnitVector", "[", 
                    RowBox[{"width", ",", "i"}], "]"}]}], "&"}], ")"}]}], ",", 
                 RowBox[{"{", "1", "}"}], ",", 
                 RowBox[{"Heads", "->", "False"}]}], "]"}], "]"}], ",", 
