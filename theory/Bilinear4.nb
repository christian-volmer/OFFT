     RowBox[{
      RowBox[{"width", "=", 
       RowBox[{
        RowBox[{"Dimensions", "[", "m", "]"}], "\[LeftDoubleBracket]", "2", 
        "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"idx", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", 
         RowBox[{"Length", "[", "m", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"list", "=", 
          RowBox[{"Complement", "[", 
           RowBox[{
            RowBox[{"Flatten", "[", 
             RowBox[{"Position", "[", 
              RowBox[{"m", ",", 
               RowBox[{"_", "?", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"Abs", "[", "#", "]"}], "==", 
                   RowBox[{"UnitVector", "[", 
                    RowBox[{"width", ",", "i"}], "]"}]}], "&"}], ")"}]}], ",", 
               RowBox[{"{", "1", "}"}], ",", 
               RowBox[{"Heads", "->", "False"}]}], "]"}], "]"}], ",", "idx"}],
            "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"list", "==", 
            RowBox[{"{", "}"}]}], ",", 
