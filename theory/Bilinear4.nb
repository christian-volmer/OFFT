 RowBox[{
  RowBox[{
   RowBox[{"ColumnOrdering", "[", "m_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{"idx", ",", "list", ",", "width"}], "\[IndentingNewLine]", 
